{
  "name": "vue-auth-code-number-input",
  "version": "1.0.2",
  "description": "Authentication code input component in vue",
  "keywords": ["vue", "authentication", "security", "code", "input", "component"],
  "author": "https://github.com/stojanovic-makro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stojanovic-makro/vue-auth-code-number-input"
  },
  "main": "dist/vue-auth-code-input.umd.js",
  "module": "dist/vue-auth-code-input.esm.js",
  "unpkg": "dist/vue-auth-code-input.min.js",
  "browser": {
    "./sfc": "src/vue-auth-code-input.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8"
  }
}
